#include <windows.h>

VS_VERSION_INFO VERSIONINFO
FILEVERSION    @_FILEVERSION@
PRODUCTVERSION @_PRODUCTVERSION@
FILEFLAGS      0x0L
FILEFLAGSMASK  0x3fL
FILEOS         VOS_NT_WINDOWS32
FILETYPE       VFT_APP
FILESUBTYPE    VFT2_UNKNOWN
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "000004b0"
            BEGIN
                VALUE "CompanyName", "@_CompanyName@"
                VALUE "FileDescription", "@_FileDescription@"
                VALUE "FileVersion", "@_FileVersion@"
                VALUE "LegalCopyright", "@_LegalCopyright@"
                VALUE "InternalName", "@_InternalName@"
                VALUE "OriginalFilename","@_OriginalFilename@"
                VALUE "ProductName", "@_InternalName@"
                VALUE "ProductVersion", "@_ProductVersion@"
            END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x0, 1200
    END
END

